<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp/etc/csp_whitelist.xsd">
    <policies>
        <policy id="img-src">
            <values>
                <value id="amazonaws" type="host">*.amazonaws.com</value>
                <value id="cloudfront" type="host">*.cloudfront.net</value>
            </values>
        </policy>
        <policy id="media-src">
            <values>
                <value id="amazonaws" type="host">*.amazonaws.com</value>
                <value id="cloudfront" type="host">*.cloudfront.net</value>
            </values>
        </policy>
        <policy id="script-src">
            <values>
                <value id="amazonaws" type="host">*.amazonaws.com</value>
                <value id="cloudfront" type="host">*.cloudfront.net</value>
            </values>
        </policy>
        <policy id="style-src">
            <values>
                <value id="amazonaws" type="host">*.amazonaws.com</value>
                <value id="cloudfront" type="host">*.cloudfront.net</value>
            </values>
        </policy>
        <policy id="connect-src">
            <values>
                <value id="amazonaws" type="host">*.amazonaws.com</value>
                <value id="cloudfront" type="host">*.cloudfront.net</value>
            </values>
        </policy>
        <policy id="font-src">
            <values>
                <value id="amazonaws" type="host">*.amazonaws.com</value>
                <value id="cloudfront" type="host">*.cloudfront.net</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
